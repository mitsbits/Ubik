<?xml version="1.0" encoding="utf-8" ?>
<groups>
  <group key="usrs" display="User Administration">
    <icon>
      <cssclass>fa fa-users</cssclass>
    </icon>
    <elements>
      <element display="Roles" href="/useradministration/roles" role="anchor" target="_self">
        <icon>
          <cssclass>fa fa fa-angle-double-right</cssclass>
        </icon>
      </element>
      <element display="Users" href="/useradministration/users" role="anchor" target="_self">
        <icon>
          <cssclass>fa fa fa-angle-double-right</cssclass>
        </icon>
      </element>
    </elements>
  </group>

  <group key="modls" display="Layouts and Modules">
    <icon>
      <cssclass>fa fa-file-code-o</cssclass>
    </icon>
    <elements>
      <element display="Layouts" href="/devices" role="anchor" target="_self">
        <icon>
          <cssclass>fa fa-file-code-o</cssclass>
        </icon>
        <elements>
          <element display="New Layout" href="/devices/new-device" role="anchor" target="_self">
            <icon>
              <cssclass>fa fa-plus</cssclass>
            </icon>
          </element>
        </elements>
      </element>
      <element display="Modules" href="/modules" role="anchor" target="_self">
        <icon>
          <cssclass>fa fa-object-group</cssclass>
        </icon>
      </element>
    </elements>
  </group>

  <group key="txnm" display="Taxonomies">
    <icon>
      <cssclass>fa fa-sitemap</cssclass>
    </icon>
    <elements>
      <element display="Divisions" href="/taxonomies/divisions" role="anchor" target="_self">
        <icon>
          <cssclass>fa fa fa-angle-double-right</cssclass>
        </icon>
      </element>
    </elements>
  </group>

  <group key="mntns" display="Maintenance">
    <icon>
      <cssclass>fa fa-cogs</cssclass>
    </icon>
    <elements>
      <element display="Error Logs" href="/errorlogs" role="anchor" target="_self">
        <icon>
          <cssclass>fa fa fa-angle-double-right</cssclass>
        </icon>
      </element>
    </elements>
  </group>
</groups>